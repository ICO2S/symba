                <!--
                    Change driver jar for appropriate database
                    For MySQL use: mysql-connector-java-X-bin.jar
                    For Hypersonic use: hsqldb.jar
                -->
                <!-- Allyson Lister (3 Jul 2007) changed this from hsqldb jar -->
                <!--<jdbc.driver.jar>${basedir}/../../../../../c/cygpath/h/.m2/repository/postgresql/postgresql/8.1-407.jdbc3/postgresql-8.1-407.jdbc3.jar</jdbc.driver.jar>-->
